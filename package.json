{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nodemon -e ts -w ./src -x ts-node ./src/main.ts",
    "start:prod": "node dist/main",
    "syncpack:format": "syncpack-format",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "ts-mocha --config .mocharc-unit.js",
    "test:e2e": "ts-mocha --config .mocharc-e2e.js"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/core": "^1.3.14",
    "@swc/register": "^0.1.10",
    "@types/express": "^4.17.13",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.6",
    "chai-subset": "^1.6.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.1.0",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "syncpack": "^8.3.9",
    "ts-loader": "^9.2.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
