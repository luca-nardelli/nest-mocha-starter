  "scripts": {
    "build": "nest build",
    "console": "node ./dist/src/console-main.js",
    "console:dev": "ts-node ./src/console-main.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nodemon -e ts -w ./src -x ts-node ./src/main.ts",
    "start:prod": "node dist/main",
    "syncpack:format": "syncpack-format",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "ts-mocha --config .mocharc-e2e.js",
    "test:unit": "ts-mocha --config .mocharc-unit.js"
  },
